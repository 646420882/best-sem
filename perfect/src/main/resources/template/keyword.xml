<?xml version="1.0" encoding="UTF-8"?>
<?mso-application progid="Excel.Sheet"?>
<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="0"/>
        <loop startRow="1" endRow="1" items="beans" var="bean"
              varType="com.perfect.entity.LexiconEntity">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">bean.category</mapping>
                <mapping row="1" col="1">bean.group</mapping>
                <mapping row="1" col="2">bean.keyword</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"></cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>
